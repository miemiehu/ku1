  v-for=\"(item,index) in kechengguanliyuanxingbieOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='kechengguanliyuan'\"  label=\"手机\" prop=\"shouji\">\r\n          <el-input v-model=\"ruleForm.shouji\"               placeholder=\"手机\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='kechengguanliyuan'\"  label=\"邮箱\" prop=\"youxiang\">\r\n          <el-input v-model=\"ruleForm.youxiang\"               placeholder=\"邮箱\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"24\">  \r\n        <el-form-item v-if=\"flag=='kechengguanliyuan'\" label=\"照片\" prop=\"zhaopian\">\r\n          <file-upload\r\n          tip=\"点击上传照片\"\r\n          action=\"file/upload\"\r\n          :limit=\"3\"\r\n          :multiple=\"true\"\r\n          :fileUrls=\"ruleForm.zhaopian?ruleForm.zhaopian:''\"\r\n          @change=\"kechengguanliyuanzhaopianUploadChange\"\r\n          ></file-upload>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-form-item v-if=\"flag=='users'\" label=\"用户名\" prop=\"username\">\r\n        <el-input v-model=\"ruleForm.username\" \r\n        placeholder=\"用户名\"></el-input>\r\n      </el-form-item>\r\n      <el-col :span=\"24\">\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onUpdateHandler\">修 改</el-button>\r\n      </el-form-item>\r\n      </el-col>\r\n      </el-row>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n<script>\r\n// 数字，邮件，手机，url，身份证校验\r\nimport { isNumber,isIntNumer,isEmail,isMobile,isPhone,isURL,checkIdCard } from \"@/utils/validate\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      ruleForm: {},\r\n      flag: '',\r\n      usersFlag: false,\r\n      yonghuxingbieOptions: [],\r\n      jiangshixingbieOptions: [],\r\n      putongguanliyuanxingbieOptions: [],\r\n      kechengguanliyuanxingbieOptions: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    var table = this.$storage.get(\"sessionTable\");\r\n    this.flag = table;\r\n    this.$http({\r\n      url: `${this.$storage.get(\"sessionTable\")}/session`,\r\n      method: \"get\"\r\n    }).then(({ data }) => {\r\n      if (data && data.code === 0) {\r\n        this.ruleForm = data.data;\r\n      } else {\r\n        this.$message.error(data.msg);\r\n      }\r\n    });\r\n    this.yonghuxingbieOptions = \"男,女\".split(',')\r\n    this.jiangshixingbieOptions = \"男,女\".split(',')\r\n    this.putongguanliyuanxingbieOptions = \"男,女\".split(',')\r\n    this.kechengguanliyuanxingbieOptions = \"男,女\".split(',')\r\n  },\r\n  methods: {\r\n    yonghuzhaopianUploadChange(fileUrls) {\r\n        this.ruleForm.zhaopian = fileUrls;\r\n    },\r\n    jiangshitouxiangUploadChange(fileUrls) {\r\n        this.ruleForm.touxiang = fileUrls;\r\n    },\r\n    putongguanliyuanzhaopianUploadChange(fileUrls) {\r\n        this.ruleForm.zhaopian = fileUrls;\r\n    },\r\n    kechengguanliyuanzhaopianUploadChange(fileUrls) {\r\n        this.ruleForm.zhaopian = fileUrls;\r\n    },\r\n    onUpdateHandler() {\r\n      if((!this.ruleForm.zhanghao)&& 'yonghu'==this.flag){\r\n        this.$message.error('账号不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.mima)&& 'yonghu'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.nicheng)&& 'yonghu'==this.flag){\r\n        this.$message.error('昵称不能为空');\r\n        return\r\n      }\r\n      if( 'yonghu' ==this.flag && this.ruleForm.shouji&&(!isMobile(this.ruleForm.shouji))){\r\n        this.$message.error(`手机应输入手机格式`);\r\n        return\r\n      }\r\n      if( 'yonghu' ==this.flag && this.ruleForm.youxiang&&(!isEmail(this.ruleForm.youxiang))){\r\n        this.$message.error(`邮箱应输入邮箱格式`);\r\n        return\r\n      }\r\n      if( 'yonghu' ==this.flag && this.ruleForm.money&&(!isNumber(this.ruleForm.money))){\r\n        this.$message.error(`余额应输入数字`);\r\n        return\r\n      }\r\n      if((!this.ruleForm.jiangshigonghao)&& 'jiangshi'==this.flag){\r\n        this.$message.error('讲师工号不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.mima)&& 'jiangshi'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.jiangshixingming)&& 'jiangshi'==this.flag){\r\n        this.$message.error('讲师姓名不能为空');\r\n        return\r\n      }\r\n      if( 'jiangshi' ==this.flag && this.ruleForm.lianxidianhua&&(!isMobile(this.ruleForm.lianxidianhua))){\r\n        this.$message.error(`联系电话应输入手机格式`);\r\n        return\r\n      }\r\n      if( 'jiangshi' ==this.flag && this.ruleForm.jiangshiyouxiang&&(!isEmail(this.ruleForm.jiangshiyouxiang))){\r\n        this.$message.error(`讲师邮箱应输入邮箱格式`);\r\n        return\r\n      }\r\n      if( 'jiangshi' ==this.flag && this.ruleForm.money&&(!isNumber(this.ruleForm.money))){\r\n        this.$message.error(`余额应输入数字`);\r\n        return\r\n      }\r\n      if((!this.ruleForm.putongzhanghao)&& 'putongguanliyuan'==this.flag){\r\n        this.$message.error('普通账号不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.mima)&& 'putongguanliyuan'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.putongxingming)&& 'putongguanliyuan'==this.flag){\r\n        this.$message.error('普通姓名不能为空');\r\n        return\r\n      }\r\n      if( 'putongguanliyuan' ==this.flag && this.ruleForm.shouji&&(!isMobile(this.ruleForm.shouji))){\r\n        this.$message.error(`手机应输入手机格式`);\r\n        return\r\n      }\r\n      if( 'putongguanliyuan' ==this.flag && this.ruleForm.youxiang&&(!isEmail(this.ruleForm.youxiang))){\r\n        this.$message.error(`邮箱应输入邮箱格式`);\r\n        return\r\n      }\r\n      if( 'putongguanliyuan' ==this.flag && this.ruleForm.money&&(!isNumber(this.ruleForm.money))){\r\n        this.$message.error(`余额应输入数字`);\r\n        return\r\n      }\r\n      if((!this.ruleForm.kechengzhanghao)&& 'kechengguanliyuan'==this.flag){\r\n        this.$message.error('课程账号不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.mima)&& 'kechengguanliyuan'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.kechengxingming)&& 'kechengguanliyuan'==this.flag){\r\n        this.$message.error('课程姓名不能为空');\r\n        return\r\n      }\r\n      if( 'kechengguanliyuan' ==this.flag && this.ruleForm.shouji&&(!isMobile(this.ruleForm.shouji))){\r\n        this.$message.error(`手机应输入手机格式`);\r\n        return\r\n      }\r\n      if( 'kechengguanliyuan' ==this.flag && this.ruleForm.youxiang&&(!isEmail(this.ruleForm.youxiang))){\r\n        this.$message.error(`邮箱应输入邮箱格式`);\r\n        return\r\n      }\r\n      if( 'kechengguanliyuan' ==this.flag && this.ruleForm.money&&(!isNumber(this.ruleForm.money))){\r\n        this.$message.error(`余额应输入数字`);\r\n        return\r\n      }\r\n      if('users'==this.flag && this.ruleForm.username.trim().length<1) {\r\n\tthis.$message.error(`用户名不能为空`);\r\n        return\t\r\n      }\r\n      this.$http({\r\n        url: `${this.$storage.get(\"sessionTable\")}/update`,\r\n        method: \"post\",\r\n        data: this.ruleForm\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.$message({\r\n            message: \"修改信息成功\",\r\n            type: \"success\",\r\n            duration: 1500,\r\n            onClose: () => {\r\n            }\r\n          });\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}